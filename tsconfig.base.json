{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.build.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "actions-kit": ["packages/actions-kit/src/index.ts"],
      "actions-kit/*": ["packages/actions-kit/src/*"],
      "@actions-kit/cli": ["packages/cli/src/index.ts"],
      "unplugin-actions-kit": ["packages/unplugin/src/index.ts"],
      "@actions-kit/action-schema": ["packages/action-schema/src/index.ts"],
      "@actions-kit/builder": ["packages/builder/src/index.ts"],
      "@actions-kit/builder/*": ["packages/builder/src/*"],
      "@actions-kit/rspack-builder": ["packages/builders/rspack-builder/src/index.ts"],
      "@actions-kit/vite-builder": ["packages/builders/vite-builder/src/index.ts"],
      "@actions-kit/esbuild-builder": ["packages/builders/esbuild-builder/src/index.ts"],

    }
  }
}
