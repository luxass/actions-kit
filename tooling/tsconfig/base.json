      "@actions-sdk/esbuild-builder": ["./packages/builders/esbuild/src/index.ts"],
      "@actions-sdk/rolldown-builder": ["./packages/builders/rolldown/src/index.ts"],
      "@actions-sdk/rollup-builder": ["./packages/builders/rollup/src/index.ts"],
      "@actions-sdk/rspack-builder": ["./packages/builders/rspack/src/index.ts"],
      "@actions-sdk/vite-builder": ["./packages/builders/vite/src/index.ts"],
