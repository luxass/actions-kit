{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./base.build.json",
  "compilerOptions": {
    "baseUrl": "../..",
    "paths": {
      "actions-kit": ["./packages/actions-kit/src/index.ts"],
      "actions-kit/builder-utils": ["./packages/actions-kit/src/builder/utils.ts"],
      "actions-kit/*": ["./packages/actions-kit/src/*"],
      "@actions-sdk/action-utils": ["./packages/action-utils/src/index.ts"],
