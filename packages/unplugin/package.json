{
  "name": "unplugin-actions-kit",
  "version": "0.3.1",
  "description": "A toolkit for GitHub Actions",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.22.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/actions-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/actions-kit.git",
    "directory": "packages/unplugin"
  },
  "bugs": {
    "url": "https://github.com/luxass/actions-kit/issues"
  },
  "keywords": [
    "actions",
    "github actions",
    "toolkit",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "rolldown",
    "rspack"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./rolldown": {
      "import": "./dist/rolldown.mjs",
      "require": "./dist/rolldown.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --tsconfig=./tsconfig.build.json",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf dist node_modules",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@actions/core": "^1.11.1",
    "@rspack/core": ">=1.0.0",
    "esbuild": ">=0.24.0",
    "rolldown": "0.12.1",
    "rollup": ">=3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:unplugin",
    "unplugin": "catalog:unplugin",
    "yaml": "catalog:unplugin",
    "zod": "catalog:prod"
  },
  "devDependencies": {
    "@actions-kit/tsconfig": "workspace:*",
    "@actions-kit/tsdown-config": "workspace:*",
    "@actions/core": "catalog:actions",
    "@luxass/eslint-config": "catalog:linting",
    "@rollup/plugin-typescript": "catalog:builders",
    "@rspack/core": "catalog:builders",
    "@types/node": "catalog:types",
    "esbuild": "catalog:builders",
    "eslint": "catalog:linting",
    "publint": "catalog:dev",
    "rolldown": "catalog:builders",
    "rollup": "catalog:builders",
    "ts-loader": "catalog:unplugin",
    "tsdown": "catalog:dev",
    "tslib": "catalog:unplugin",
    "typescript": "catalog:dev",
    "vite": "catalog:builders",
    "vitest": "catalog:testing",
    "vitest-testdirs": "catalog:testing",
    "webpack": "catalog:builders"
  }
}
