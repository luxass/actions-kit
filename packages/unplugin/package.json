{
  "name": "unplugin-actions-kit",
  "version": "0.2.3",
  "description": "A toolkit for GitHub Actions",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/actions-kit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/actions-kit.git",
    "directory": "packages/unplugin"
  },
  "bugs": {
    "url": "https://github.com/luxass/actions-kit/issues"
  },
  "keywords": [
    "actions",
    "github actions",
    "toolkit",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "rolldown",
    "rspack"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./rolldown": {
      "import": "./dist/rolldown.js",
      "require": "./dist/rolldown.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --tsconfig=./tsconfig.build.json",
    "dev": "tsdown --watch",
    "clean": "git clean -xdf dist node_modules",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@actions/core": "^1.11.1",
    "@rspack/core": ">=1.0.0",
    "esbuild": ">=0.24.0",
    "rolldown": "0.12.1",
    "rollup": ">=3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:",
    "js-yaml": "catalog:",
    "unplugin": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@actions-kit/tsconfig": "workspace:*",
    "@actions-kit/tsdown-config": "workspace:*",
    "@actions/core": "catalog:",
    "@luxass/eslint-config": "catalog:linting",
    "@rollup/plugin-typescript": "catalog:",
    "@rspack/core": "catalog:",
    "@types/js-yaml": "catalog:types",
    "@types/node": "catalog:types",
    "esbuild": "catalog:",
    "eslint": "catalog:linting",
    "publint": "catalog:dev",
    "rolldown": "catalog:",
    "rollup": "catalog:",
    "ts-loader": "catalog:",
    "tsdown": "catalog:dev",
    "tslib": "catalog:",
    "typescript": "catalog:dev",
    "vite": "catalog:",
    "vitest": "catalog:testing",
    "vitest-testdirs": "catalog:testing",
    "webpack": "catalog:"
  }
}
